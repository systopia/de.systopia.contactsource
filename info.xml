<?xml version="1.0"?>
<extension key="de.systopia.contactsource" type="module">
  <file>contactsource</file>
  <name>Contact Source</name>
  <description>More detailed data structures for contact sources</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.contactsource</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.contactsource</url>
    <url desc="Support">https://github.com/systopia/de.systopia.contactsource/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate></releaseDate>
  <version>0.5-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <comments/>
  <civix>
    <namespace>CRM/Contactsource</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Contactsource_Upgrader</upgrader>
</extension>
